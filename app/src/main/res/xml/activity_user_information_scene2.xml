<?xml version="1.0" encoding="utf-8"?>
<MotionScene 
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:motion="http://schemas.android.com/apk/res-auto">

    <Transition
        motion:constraintSetEnd="@+id/end"
        motion:constraintSetStart="@id/start"
        motion:duration="1000">
       <KeyFrameSet>
       </KeyFrameSet>
    </Transition>

    <ConstraintSet android:id="@+id/start">
        <Constraint
            android:layout_height="wrap_content"
            android:layout_width="wrap_content"
            android:id="@+id/btnNext"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintTop_toBottomOf="@+id/etHeight"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintVertical_bias="0.516"
            motion:layout_constraintEnd_toEndOf="parent" />
        <Constraint
            motion:layout_constraintVertical_bias="0.097"
            android:layout_height="30dp"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintTop_toTopOf="parent"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintHorizontal_bias="0.498"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="210dp"
            android:id="@+id/spinner1" />
        <Constraint
            android:layout_height="30dp"
            android:layout_width="210dp"
            android:id="@+id/spinner2"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintTop_toBottomOf="@+id/spinner1"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintEnd_toEndOf="parent"
            motion:layout_constraintVertical_bias="0.157"
            motion:layout_constraintHorizontal_bias="0.498" />
        <Constraint
            android:layout_height="wrap_content"
            android:layout_width="wrap_content"
            android:id="@+id/etAge"
            motion:layout_constraintTop_toBottomOf="@+id/spinner2"
            android:layout_marginTop="88dp"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintEnd_toEndOf="parent"
            motion:layout_constraintVertical_bias="0.043"
            motion:layout_constraintHorizontal_bias="0.498" />
        <Constraint
            android:layout_height="wrap_content"
            android:layout_width="wrap_content"
            android:id="@+id/etWeight"
            motion:layout_constraintTop_toBottomOf="@+id/etAge"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintEnd_toEndOf="parent"
            motion:layout_constraintVertical_bias="0.251"
            motion:layout_constraintHorizontal_bias="0.498" />
        <Constraint
            android:layout_height="wrap_content"
            android:layout_width="wrap_content"
            android:id="@+id/etHeight"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintEnd_toEndOf="parent"
            motion:layout_constraintHorizontal_bias="0.498"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintTop_toBottomOf="@+id/etWeight"
            motion:layout_constraintVertical_bias="0.299" />
        <Constraint
            android:layout_height="wrap_content"
            android:layout_width="wrap_content"
            android:id="@+id/textView3"
            motion:layout_constraintEnd_toEndOf="parent"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintBottom_toTopOf="@+id/spinner1"
            motion:layout_constraintTop_toTopOf="parent" />
        <Constraint
            android:layout_height="wrap_content"
            android:layout_width="wrap_content"
            android:id="@+id/textView4"
            motion:layout_constraintEnd_toEndOf="parent"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintBottom_toTopOf="@+id/etAge"
            motion:layout_constraintTop_toBottomOf="@+id/spinner2" />
        <Constraint
            android:layout_height="26dp"
            android:layout_width="44dp"
            android:id="@+id/textView5"
            motion:layout_constraintTop_toBottomOf="@+id/etAge"
            motion:layout_constraintBottom_toTopOf="@+id/etWeight"
            motion:layout_constraintEnd_toEndOf="parent"
            motion:layout_constraintStart_toStartOf="parent" />
        <Constraint
            android:layout_height="wrap_content"
            android:layout_width="wrap_content"
            android:id="@+id/textView6"
            motion:layout_constraintEnd_toEndOf="parent"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintBottom_toTopOf="@+id/spinner2"
            motion:layout_constraintTop_toBottomOf="@+id/spinner1" />
        <Constraint
            android:layout_height="wrap_content"
            android:layout_width="wrap_content"
            android:id="@+id/textView7"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintEnd_toEndOf="parent"
            motion:layout_constraintTop_toTopOf="@+id/etWeight"
            android:layout_marginTop="74dp"
            motion:layout_constraintBottom_toTopOf="@+id/etHeight"
            motion:layout_constraintVertical_bias="0.043" />
        <Constraint
            motion:layout_editor_absoluteY="123dp"
            android:layout_height="36dp"
            motion:layout_editor_absoluteX="26dp"
            android:layout_width="112dp"
            android:id="@+id/checkBox" />
        <Constraint
            motion:layout_editor_absoluteY="176dp"
            android:layout_height="wrap_content"
            motion:layout_editor_absoluteX="26dp"
            android:layout_width="wrap_content"
            android:id="@+id/checkBox2" />
        <Constraint
            motion:layout_editor_absoluteY="176dp"
            android:layout_height="wrap_content"
            motion:layout_editor_absoluteX="26dp"
            android:layout_width="wrap_content"
            android:id="@+id/cbFemale" />
        <Constraint
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            motion:layout_editor_absoluteY="133dp"
            android:id="@+id/cbMale"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintEnd_toEndOf="parent"
            motion:layout_constraintTop_toBottomOf="@+id/spinner1"
            motion:layout_constraintBottom_toTopOf="@+id/textView6" />
    </ConstraintSet>

    <ConstraintSet android:id="@+id/end">
    </ConstraintSet>
</MotionScene>